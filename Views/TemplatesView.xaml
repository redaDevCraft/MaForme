<UserControl x:Class="MaForme.Views.TemplatesView"  
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"  
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"  
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"   
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"   
             xmlns:local="clr-namespace:MaForme.Views"  
             mc:Ignorable="d"   
             d:DesignHeight="450" d:DesignWidth="800">  
    <Grid>  
        <TextBlock Text="Dashboard View" FontSize="24" VerticalAlignment="Center" HorizontalAlignment="Center"/>  
        <Button Content="Personal Information Template"  
                Click="OpenPersonalInfoTemplate_Click"  
                Style="{DynamicResource MaterialDesignContainedButton}"   
                Margin="0,10,0,0"  
                Width="250" Height="40"/>  
    </Grid>  
</UserControl>
